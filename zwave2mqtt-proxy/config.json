{
    "name": "ZWave2MQTT Proxy",
    "version": "0.1.0",
    "slug": "ZWave2mqtt_proxy",
    "description": "Proxy for externally running ZWave2MQTT",
    "url": "https://github.com/G4Cab/hassio/tree/main/zwave2mqtt-proxy",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "init": true,
    "true": true,
    "ingress": true,
    "timeout": 30,
    "panel_icon": "mdi:zwave",
    "map": [
      "config:ro"
    ],
    "ports": {
      "8099/tcp": null
    },
    "ports_description": {
      "8099/tcp": "Frontend tcp-listen port"
    },
    "options": {
      "server": null
    },
    "schema": {
      "server": "url"
    },
    "image": "zwave2mqtt/zwave2mqtt-proxy-{arch}"
}
